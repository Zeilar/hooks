		"eslint-plugin-react-hooks": "2.5.1",
		"husky": "7.0.4",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"size-limit": "7.0.8",
		"tsdx": "0.14.1",
		"tslib": "2.3.1",
		"typescript": "4.6.3",
		"@testing-library/react-hooks": "7.0.2"
	}
